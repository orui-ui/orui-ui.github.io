{
  "private": false,
  "name": "orui-ui",
  "version": "1.0.3",
  "description": "一款用于快速构建React应用程序的高性能、轻量级UI组件库。",
  "homepage": "https://github.com/orui-ui/orui-ui.github.io/tree/master | https://gitee.com/orui-ui/orui-ui",
  "bugs": {
    "url": "https://github.com/orui-ui/orui-ui.github.io/tree/master | https://gitee.com/orui-ui/orui-ui",
    "email": "3399251596@qq.com"
  },
  "keywords": [
    "ui",
    "react",
    "组件",
    "好用"
  ],
  "engines": {},
  "author": {
    "name": "Rui Xiao",
    "email": "3399251596@qq.com"
  },
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build:c": "pnpm -r --filter=./packages/** run build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build",
    "release": "npm publish",
    "release:c": "pnpm -r --filter=./packages/** run release"
  },
  "files": [
    "bin",
    "README.md",
    "README.zh-CN.md",
    "package.json"
  ],
  "main": "bin/index.ts",
  "module": "bin/index.js",
  "typings": "bin/index.d.ts",
  "type": "module",
  "dependencies": {
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.0.3",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.0",
    "gh-pages": "^3.0.0",
    "lerna": "^6.0.1",
    "lint-staged": "^10.0.7",
    "react": "^18.0.0",
    "yorkie": "^2.0.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
