{
  "private": false,
  "name": "orui-ui",
  "version": "1.0.0",
  "description": "一款用于快速构建React应用程序的高性能、轻量级UI组件库。",
  "homepage": "https://github.com/xiaoruil/xiaoruil.github.io | https://gitee.com/xiaoruil/orui-ui",
  "bugs": {
    "url": "https://github.com/xiaoruil/xiaoruil.github.io | https://gitee.com/xiaoruil/orui-ui",
    "email": "3399251596@qq.com"
  },
  "keywords": [
    "ui",
    "react",
    "组件",
    "好用"
  ],
  "engines": {},
  "author": {
    "name": "Rui Xiao",
    "email": "3399251596@qq.com"
  },
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": " rollup -c",
    "build:c": "pnpm -r --filter=./packages/** run build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build",
    "release": "npm publish"
  },
  "files": [
    "bin"
  ],
  "main": "bin/index.ts",
  "module": "bin/index.js",
  "typings": "bin/index.d.ts",
  "type": "module",
  "dependencies": {
    "lerna": "^6.0.1",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.0.3",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "autoprefixer": "^10.4.13",
    "dumi": "^1.1.0",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "postcss": "^8.4.18",
    "prettier": "^2.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "yorkie": "^2.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "browserslist": [
    "defaults",
    "not ie < 8",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
